<div id="<%= dom_id post %>">
  <p>
    <strong>Tittle:</strong>
    <%= post.tittle %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= post.content %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= post.status %>
  </p>

  <p>
    <strong>User:</strong>
    <%= post.user_id %>
  </p>

  <p>
    <strong>Post:</strong>
    <%= post.post_id %>
  </p>

</div>
